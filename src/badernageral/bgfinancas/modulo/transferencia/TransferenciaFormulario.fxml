            <Label fx:id="labelContaOrigem" focusTraversable="false" text="Conta origem:" GridPane.rowIndex="1" />
            <ComboBox fx:id="contaOrigem" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="labelContaDestino" focusTraversable="false" text="Conta destino:" GridPane.rowIndex="2" />
